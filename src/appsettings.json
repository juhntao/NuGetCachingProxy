{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "NuGetRoute": {
        "ClusterId": "NuGetCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "NuGetCluster": {
        "Destinations": {
          "ApiNuGetOrg": {
            "Address": "https://api.nuget.org/"
          }
        }
      }
    }
  },
  "ServiceConfig": {
    "DatabaseName": "package-cache"
  }
}
